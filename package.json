{
  "name": "opendraft-pt-recovery",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "node --test",
    "check": "npm run test && npm run lint",
    "pipeline:session": "node scripts/process-transcript.mjs examples/transcripts/sample-pt-transcript.txt examples/output/generated-session.json",
    "pipeline:notes": "node scripts/generate-dual-notes.mjs examples/output/generated-session.json examples/output/dual-notes.json examples/transcripts/sample-pt-transcript.txt",
    "pipeline:dashboard": "node scripts/update-dashboard-data.mjs examples/output/generated-session.json examples/output/dual-notes.json src/data/dashboard-sync.json",
    "pipeline:all": "npm run pipeline:session && npm run pipeline:notes && npm run pipeline:dashboard"
  },
  "dependencies": {
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
